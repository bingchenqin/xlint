+
+@testable import App
+import ExternalDependency
+import ExternalDependency2
+import TestAutomation
+import CoreData
+import AnotherKit
+
+class ABCDEFGHIJKLMNOPQRSTUVWX ExternalDependencySpec {
+    override func spec() {
+        describe("TableViewController") {
+            context("with graphics") {
+                var tasks: TableViewController!
+                beforeEach {
+                    let id = "1811031"
+                    let person = person(credentials: .person1)
+                    let login = person.login
+                    let context = try! login.enrollmentcontext()
+                    let data = data.build(context, id: id)
+                    tasks = try! TableViewController(login: login, id: data.id) { _ in }
+                }
+
+                describe(".viewWillAppear") {
+                    context("when header has been selected") {
+                        beforeEach {
+                            tasks.header.tableView.selectRowAtIndexPath(NSIndexPath(forRow: 0, inSection: 0), animated: false, scrollPosition: .None)
+                            expect(tasks.header.tableView.indexPathsForSelectedRows).toNot(beEmpty())
+                        }
+
+                        it("it clears selection") {
+                            tasks.viewWillAppear(false)
+                            expect(tasks.header.tableView.indexPathsForSelectedRows).toEventually(beNil())
+                        }
+                    }
+
+                }
+            }
+        }
+    }
+}
